{
  "name": "alp",
  "version": "1.0.6",
  "description": "Advanced Log Parser",
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npx tsc & electron-forge start",
    "electron": "npx tsc & electron ./dist",
    "compile": "npx tsc",
    "package": "npx tsc & electron-forge package",
    "make": "npx tsc & electron-forge make",
    "publish": "npx tsc & electron-forge publish"
  },
  "author": "HiKTO",
  "license": "ISC",
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.67",
    "@electron-forge/maker-deb": "^6.0.0-beta.67",
    "@electron-forge/maker-rpm": "^6.0.0-beta.67",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.67",
    "@electron-forge/maker-zip": "^6.0.0-beta.67",
    "electron": "^21.1.1",
    "electron-rebuild": "^3.2.9"
  },
  "dependencies": {
    "@electron-forge/publisher-github": "^6.0.0-beta.70",
    "@types/sqlite3": "^3.1.8",
    "electron-squirrel-startup": "^1.0.0",
    "git": "^0.1.5",
    "node-gyp": "^9.3.0",
    "sqlite3": "^5.1.2",
    "typescript": "^4.8.4"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "icon.ico",
        "executableName": "ALP",
        "asar": false
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "ALP"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "repository": {
              "owner": "HiKT0",
              "name": "alp"
            },
            "prerelease": true
          }
        }
      ]
    }
  }
}
